{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "targets": "> 0.25%, not dead"
        },
        "modules": false,
        "useBuiltIns": true,
        "debug": true

      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "classic"
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "isTSX": true,
        "allExtensions": true
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3
      }
    ]
  ],
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-react",
          {
            "development": true
          }
        ]
      ]
    }
  }
}

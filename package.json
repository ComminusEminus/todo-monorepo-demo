{
	"name": "todomonorepo",
	"version": "1.0.0",
	"description": "monorepo for full stack todo app",
	"keywords": [],
	"author": "Mikael Mirza",
	"license": "ISC",
	"workspaces": [
		"./packages/**/*"
	],
	"scripts": {
		"dry": "tsc --build --dry",
		"clean": "tsc --build --clean",
		"build": "tsc --build --verbose",
		"watch": "tsc --build --watch",
		"qb": "npm run build -w @api/middlewares",
		"apibuild": " npm run build -w @api/controllers",
		"corebuild": "npm run build -w @core/",
		"designbuild": "npm run build -w @design/input",
		"uibuild": "npm run build -w @ui/user",
		"uibundle": "npm run bundlebuild -w ./packages/ui/app",
		"uilaunch": "npm run start -w ./packages/ui/app",
		"apilaunch": "npm run start -w ./packages/api/server",
		"test": "jest --watchAll --coverage --verbose --silent --runInBand"
	},
	"config": {
		"mongodbMemoryServer": {
			"downloadDir": "./node_modules/mongodb-memory-server"
		}
	},
	"dependencies": {
		"@babel/helper-module-imports": "^7.22.5",
		"@babel/runtime-corejs3": "^7.22.6",
		"@hookform/resolvers": "^3.1.1",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/passport": "^1.0.12",
		"@types/passport-jwt": "^3.0.8",
		"@types/passport-local": "^1.0.35",
		"@types/styled-components": "^5.1.26",
		"axios": "^1.3.5",
		"body-parser": "^1.20.2",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"jest": "^29.6.1",
		"mongodb": "^5.6.0",
		"mongodb-memory-server": "^8.13.0",
		"mongoose": "^7.3.0",
		"node": "^20.5.0",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.1",
		"passport-local": "^1.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.44.3",
		"styled-components": "^5.3.9",
		"supertest": "^6.3.3",
		"terser-webpack-plugin": "^5.3.9",
		"yup": "^1.2.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.21.3",
		"@babel/plugin-transform-runtime": "^7.22.9",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-flow": "^7.22.5",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.22.5",
		"@types/jest": "^29.5.2",
		"@types/node": "^20.2.5",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"axios": "^1.3.5",
		"babel-loader": "^9.1.2",
		"clean-webpack-plugin": "^4.0.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.5.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"styled-components": "^5.3.11",
		"ts-loader": "^9.4.3",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.1.3",
		"webpack": "^5.76.2",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^4.13.1"
	}
}

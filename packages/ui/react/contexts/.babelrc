{
    "presets": [
        "@babel/env", 
        "@babel/react", 
        "@babel/preset-typescript"
    ],
    "plugins": [
        "@babel/plugin-transform-class-properties"
    ],
    "ignore": [
        "./src/**/*/__tests__/*",
        "./src/**/*/__stories__/*"    
    ],
    "env": {
        "test": {
            "plugins": [
                "@babel/plugin-transform-modules-commonjs"
            ]
        }
    }
  }
  
{
	"name": "@ui/form-display",
	"version": "1.0.0",
	"description": "Context provider for shared states of user interface",
	"exports":  {
		".": {
			"import": ["./src/index.ts"],
			"require": ["./dist/index.js"]
		}
	},
	"types": "types/packages/ui/form_display/src/index.d.ts",
	"dependencies" : {
		"@core/di" : "^1.0.0",
		"@core/entities" : "^1.0.0",
		"@ui/todo-list" : "^1.0.0",
		"@ui/todo-lists" : "^1.0.0",
		"@ui/contexts" : "^1.0.0"
	},
	"scripts": {
		"transpile": "babel -d dist/ --extensions .ts,.tsx src/",
		"emitDeclarations": "tsc --declaration --emitDeclarationOnly",
		"build": "npm run emitDeclarations && npm run transpile",
		"check-types": "tsc --noEmit"
	},
	"type": "module",
	"module": "dist/src/index.js",
    "main": "dist/src/index.js"
}	
